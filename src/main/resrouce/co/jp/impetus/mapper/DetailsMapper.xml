<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="co.jp.impetus.mapper.DetailsMapper">

    <select id="select" resultType="co.jp.impetus.dto.DetailsDto">
        SELECT
          isbn_cd                                         -- ISBNコード
          , production_name                               -- 作品名
          , author_name                                   -- 作者名
          , publisher                                     -- 発行元
          , issued_date                                   -- 発行年月日
        FROM
          m_book
    </select>

</mapper>